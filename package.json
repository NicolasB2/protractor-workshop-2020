{
  "name": "workshop-protractor",
  "version": "1.0.0",
  "description": "This is a Workshop about Protractor",
  "main": "index.js",
  "scripts": {
    "clean": "rm -r dist && mkdir dist",
    "build": "npm run lint && npm run clean && tsc",
    "test:local": "npm run build && protractor dist/protractor/local.config.js",
    "test:headless": "npm run build && protractor dist/protractor/headless.config.js",
    "test": "npm run test:headless",
    "postinstall": "webdriver-manager update --gecko false",
    "lint": "tslint --project tsconfig.json protractor/**/*.ts test/**/*.ts src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasB2/ProtractorWorkshop2020.git"
  },
  "keywords": [
    "ui-testing",
    "dojo",
    "practice",
    "protractor"
  ],
  "author": "Nicolas Biojo Bermeo <nicolas.biojo@correo.icesi.edu.co> (https://github.com/NicolasB2)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NicolasB2/ProtractorWorkshop2020/issues"
  },
  "homepage": "https://github.com/NicolasB2/ProtractorWorkshop2020#readme",
  "dependencies": {
    "protractor": "^7.0.0"
  },
  "devDependencies": {
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.0.25",
    "jasmine-spec-reporter": "^5.0.2",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.9.7"
  }
}
